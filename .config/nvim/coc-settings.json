{
  "codeLens.enable": true,
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": ["json", "python"],
  "languageserver": {
    "golang": {
      "command": "go-langserver",
      "filetypes": ["go"],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    }
  }
}
